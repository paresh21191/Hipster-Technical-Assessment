{
  "name": "user-discounts/user-discounts",
  "description": "A reusable, testable discount system for users in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Paresh Patanvadiya",
      "email": "pareshpatanwadiya@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "UserDiscounts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "UserDiscounts\\UserDiscountsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "version": "1.0.0"
}
